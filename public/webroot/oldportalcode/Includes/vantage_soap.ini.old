[config]
include_path=c:\inetpub\wwwroot\web\web\Includes
username=manager
password=manager

[GetPartOnHandWhse]
url=http://adel-web/VantageServices/PartOnHandWhseService.asmx?wsdl
action=GetPartOnHandWhse
params="CompanyID,cPartNum,cPlant"
result_type=GetPartOnHandWhseResult
